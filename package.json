{
  "name": "react-route-guard",
  "version": "1.0.2",
  "description": "Add powerful Route Guard functionality to `react-router-dom`",
  "main": "index.js",
  "repository": "https://github.com/pixelfusion/react-route-guard.git",
  "author": "Wison <wison@pixelfusion.co.nz>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf src/route-guard.js && ./node_modules/.bin/tsc --outFile src/route-guard.js && mv src/route-guard.js ./index.js",
    "build-declarationf-file": "rm -rf src/enhanced-route.d.ts && ./node_modules/.bin/tsc -d && mv src/enhanced-route.d.ts ./index.d.ts"
  },
  "devDependencies": {
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router-dom": "^4.0.7",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "rxjs": "^5.4.3"
  }
}
